package prOrdVirtual;

import java.util.List;
import java.util.Queue;

public class Bus {

	Queue<Operacion> IOperaciones;

	public void setOperaciones(List<Operacion> l) {

		for (int i = 0; i < l.size(); i++) {
			IOperaciones.add(l.get(i));
		}
	}

	public Operacion getOperacion() {

		return IOperaciones.remove();
	}

	public int size() {

		return IOperaciones.size();
	}
}
